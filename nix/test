{ ... }: {
	inputs = {
		pkgs = {
			url = "https://qweqweqwe.com/pkgs";
			version = "unstable";
		};

		os = {
			url = "https://qweqweqwe.com/os";
			version = "unstable";
		};
	};

	output = { pkgs, os, ... }: let
		system = "x86_64-linux";
	in {
		time.timeZone = "Europe/Moscow";
		network.hostname = "qweos";
		locale.locale = "ru_RU.UTF-8";

		users = {
			qwe = {
				groups = [ "wheel" ];
				home = '/home/qwe';
				shell = pkgs.zsh;
				import './users/qwe';
			};
		};

		environment.systemPackages = with pkgs; [
			vim,
			lshw,
		];

		services.printing.enable = true;

		boot.loader.grub.enabled = true;
		boot.loader.efi.support = true;
		boot.loader.mbr.support = false;
		boot.loader.efi.efiMountPoint = '/boot/efi';
	};
}
